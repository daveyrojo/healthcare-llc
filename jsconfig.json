{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"checkJs": true,
		"jsx": "react",
		"resolveJsonModule": true,
		"module": "commonJS",
		"removeComments": true
	},
	"exclude": ["**/node_modules"]
}
